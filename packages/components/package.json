{
  "name": "@lun/components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./elements-types-vue": {
      "types": "./dist/elements-types-vue.d.ts"
    },
    "./elements-types-react": {
      "types": "./dist/elements-types-react.d.ts"
    },
    "./elements-types-html": {
      "types": "./dist/elements-types-html.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build:dev": "cross-env NODE_ENV=development vite build",
    "build:prod": "cross-env NODE_ENV=production vite build",
    "build": "rimraf ./dist && pnpm build:dev && pnpm build:prod && node ./generateModuleTypes.js",
    "build:noType": "cross-env NO_TYPE_EMIT=true pnpm build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@floating-ui/utils": "^0.1.6",
    "@floating-ui/vue": "^1.0.6",
    "@lun/core": "workspace:^",
    "@lun/utils": "workspace:^"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@floating-ui/core": "^1.6.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "cross-env": "^7.0.3",
    "postcss-logical": "^7.0.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vite": "^4.5.2",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^0.34.6",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  }
}
