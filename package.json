{
  "name": "lun",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vitepress dev src/docs",
    "build:noType": "pnpm --filter \"./packages/**\" run build:noType",
    "build": "pnpm --filter \"./packages/**\" run build",
    "docs:gen": "node ./utils/generateAndLinkDocs.js no-symlink",
    "docs:build": "node ./utils/generateAndLinkDocs.js && node ./buildDoc.js",
    "docs:preview": "vitepress preview src/docs",
    "test": "vitest",
    "pre-version": "pnpm --filter \"./packages/**\" version prerelease --preid alpha"
  },
  "dependencies": {
    "@giscus/vue": "^3.0.0",
    "@lun/components": "workspace:^",
    "@lun/core": "workspace:^",
    "@lun/react": "workspace:^",
    "@lun/theme": "workspace:^",
    "@lun/utils": "workspace:^",
    "@vueuse/core": "^10.10.0",
    "esbuild-wasm": "^0.21.4",
    "monaco-editor": "^0.49.0",
    "react": "rc",
    "react-dom": "rc",
    "vue": "^3.4.27"
  },
  "overrides": {
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc"
  },
  "devDependencies": {
    "@lun/plugins": "workspace:^",
    "@rollup/plugin-replace": "^5.0.5",
    "@types/dom-view-transitions": "^1.0.4",
    "@types/node": "^20.13.0",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/runtime-dom": "^3.4.27",
    "cross-env": "^7.0.3",
    "cypress": "^13.10.0",
    "happy-dom": "^13.10.1",
    "new-javascript": "^0.4.3",
    "postcss-logical": "^7.0.1",
    "rimraf": "^5.0.7",
    "sass": "^1.77.4",
    "typescript": "^5.4.5",
    "v-lazy-show": "^0.2.4",
    "vite": "^5.2.12",
    "vite-plugin-dts": "^3.9.1",
    "vitepress": "1.2.2",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.19"
  },
  "resolutions": {
    "vite": "^5.2.12",
    "@vueuse/core": "^10.9.0",
    "postcss": "^8.4.30",
    "typescript": "^5.4.5",
    "vue": "^3.4.21",
    "@floating-ui/utils": "^0.2.1",
    "@floating-ui/dom": "^1.6.3"
  }
