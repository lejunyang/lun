{
  "compilerOptions": {
    "baseUrl": ".",
    "composite": true,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "types": [
      "./packages/components/dist/elements-types-vue.d.ts",
      "vite/client",
      "./packages/components/src/types/vue-type-fix.d.ts"
    ],
    "paths": {
      "data": ["./src/utils/data.ts"],
      "@lun/components": ["./packages/components/index.ts"],
      "@lun/theme/custom": ["./packages/theme/src/custom/custom.ts"],
      "@lun/core/date-dayjs": ["./packages/core/src/presets/date.dayjs.ts"],
      "@lun/core": ["./packages/core/index.ts"]
    }
  },
  "include": [
    "./src/docs/**/*.vue.tsx",
    "./src/components/*.vue",
    "./src/docs/**/*.vue",
    "./src/docs/.vitepress/theme/Layout.vue",
    "./src/utils/*.ts",
    "./src/docs/.vitepress/**/*.ts",
    "./packages/components/dist/elements-types-vue.d.ts",
    "./packages/core/src/presets/date.dayjs.ts",
    "./packages/theme/src/custom/custom.ts",
    "./vite.config.ts"
